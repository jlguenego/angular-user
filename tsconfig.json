{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@jlguenego/angular-layout": [
        "projects/layout/src/public_api.ts"
      ],
      "@jlguenego/angular-user": [
        "projects/user/src/public_api.ts"
      ],
      "@jlguenego/angular-user-firebase": [
        "projects/firebase/src/public_api.ts"
      ],
      "layout": [
        "dist/layout"
      ],
      "layout/*": [
        "dist/layout/*"
      ],
      "user": [
        "dist/user"
      ],
      "user/*": [
        "dist/user/*"
      ],
      "firebase": [
        "dist/firebase"
      ],
      "firebase/*": [
        "dist/firebase/*"
      ]
    }
  }
}